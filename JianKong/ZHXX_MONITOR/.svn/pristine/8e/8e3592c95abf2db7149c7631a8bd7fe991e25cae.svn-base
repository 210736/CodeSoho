using System;
using System.Collections.Generic;
using System.Text;

namespace MonitorAndSet.TopoInfo
{
    /// <summary>
    /// TaskAttribute表示任务属性
    /// </summary>
    public class TaskAttribute
    {
        public TaskAttribute()
        {

        }

        public TaskAttribute(int taskid, String taskname)
        {
            this.taskid = taskid;
            this.taskname = taskname;
        }

        private int m_taskid;
        /// <summary>
        /// 任务ID号
        /// </summary>
        public int taskid
        {
            get { return m_taskid; }
            set { m_taskid = value; }
        }

        private String m_taskname;
        /// <summary>
        /// 任务名称
        /// </summary>
        public String taskname
        {
            get { return m_taskname; }
            set { m_taskname = value; }
        }
    }
}
